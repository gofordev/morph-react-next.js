{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "extends": [
        "prettier"
    ],
    "rules": {
        "no-console": 0,
        "react/jsx-uses-vars": 1,
        "prettier/prettier": ["error", {"semi": false}]
    }
}
